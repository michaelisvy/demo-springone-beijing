<html xmlns:c="http://java.sun.com/jsp/jstl/core"  xmlns:spring="http://www.springframework.org/tags" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:form="http://www.springframework.org/tags/form"xmlns:custom="urn:jsptagdir:/WEB-INF/tags">	<jsp:output doctype-root-element="HTML" doctype-system="about:legacy-compat" />	<jsp:directive.page contentType="text/html;charset=UTF-8" />  	<head>		<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1"/>		<title>Create a new User</title>				<custom:script url="/data/webjars/jquery/1.8.3/jquery.min.js"/>		<custom:script url="/data/webjars/jquery-ui/1.9.1/js/jquery-ui-1.9.1.custom.js"/>			<custom:stylesheet url="/data/webjars/bootstrap/2.2.1/css/bootstrap.min.css"/>					</head>	<body>		<jsp:include page="menuBootstrap.jspx"/>		<div class="container" style="padding-top: 50px;">				<custom:form modelAttribute="user" formUrl="/data/userCustomTag.htm" id="add-user-form">				<custom:inputField label="First Name" name="firstName"/>				<custom:inputField label="Last Name" name="lastName"/>				<div class="form-actions">					<button type="submit" class="btn btn-primary">Save changes</button>					<button type="reset" class="btn">Cancel</button>				</div>			</custom:form>		</div>	</body></html>