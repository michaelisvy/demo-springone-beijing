<html xmlns:c="http://java.sun.com/jsp/jstl/core"  xmlns:spring="http://www.springframework.org/tags" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:form="http://www.springframework.org/tags/form"xmlns:datatables="http://github.com/datatables4j">	<jsp:output doctype-root-element="HTML" doctype-system="about:legacy-compat" />	<jsp:directive.page contentType="text/html;charset=UTF-8" />  	<head>		<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1"/>		<title>Create a new User</title>		<c:url value="/style" var="styleUrl" />		<link href="${styleUrl}/app.css" rel="stylesheet"/>		<c:url value="/data/webjars/bootstrap/2.2.1/css/bootstrap.min.css" var="bootstrapcss" />		<link href="${bootstrapcss}" rel="stylesheet"/>		<c:url value="/data/webjars/jquery/1.8.3/jquery.min.js" var="jquery" />		<script src="${jquery}"> <!--  do not close --> </script>				<c:url value="/data/webjars/jquery-ui/1.9.1/js/jquery-ui-1.9.1.custom.js" var="jqueryui" />		<script src="${jqueryui}"> <!--  do not close --></script>				<c:url value="/data/webjars/datatables/1.9.4/media/js/jquery.dataTables.js" var="datatables" />		<script src="${datatables}"> <!--  do not close --></script>				<c:url value="/data/webjars/datatables/1.9.4/media/css/jquery.dataTables_themeroller.css" var="datatablescss" />		<link href="${datatablescss}" rel="stylesheet"/>						<c:url value="/data/webjars/jquery-ui/1.9.1/css/smoothness/jquery-ui-1.9.1.custom.css" var="jqueryuicss" />		<link href="${jqueryuicss}" rel="stylesheet"/>	    	</head><body>	<div class="container" style="padding-top: 50px;">		<jsp:include page="menu.jspx"/>				<table border="1" style="width:50%">			<thead>				<tr>					<td>First Name</td>					<td>Last Name</td>				</tr>			</thead>			<tbody>				<c:forEach items="${userList}" var="user">					<tr>						<td>${user.firstName}</td>						<td>${user.lastName}</td>					</tr>				</c:forEach>			</tbody>		</table>				<br/><br/><br/><br/>			<hr />	<hr /><hr />	<br/><br/><br/><br/>				<datatables:table data="${userList}" id="dataTable" row="user">		   <datatables:column title="First Name" property="firstName" sortable="true" />		   <datatables:column title="Last Name" property="lastName" sortable="true"/>		</datatables:table>	</div></body></html>